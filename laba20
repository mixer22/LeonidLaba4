#include <iostream>
#include <math.h>
using namespace std;

int fact(int k)
{
	int out = 1, i = 1;
	while (i <= k)
	{
		out *= i;
		i++;
	}
	return out;
}

int main()
{
	double out = 1;
	int  k = 1, tmp = 0, x = 0, i =0;

	cout << "input X, i: ";
	cin >> x;
	cin >> i;
	while (k < 11)
	{
		out *= (x * tan(x)  + i * i) / fact(k);
		k++;
	}
	cout << out;
}
